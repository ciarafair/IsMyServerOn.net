{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ES2020",
		"jsx": "react",
		"strictNullChecks": true,
		"strictFunctionTypes": true
	},
	"exclude": [
		"node_modules",
		"**/node_modules/*"
	],
	"baseUrl": "/",
	"compileOnSave": true,
	"allowSyntheticDefaultImports": true,
	"allowJs": true,
	"checkJs": true,
	"buildOptions": {
		"outDir": "build"
	}
}
