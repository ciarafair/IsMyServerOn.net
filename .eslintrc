{
	"env": {
				"browser": true,
				"es6": true
			},
			"extends": [
				"eslint:recommended",
				"plugin:@typescript-eslint/recommended",
				"plugin:react/recommended"
			],
			"parser": "@typescript-eslint/parser",
			"parserOptions": {
				"ecmaVersion": "latest",
				"sourceType": "module"
			},
			"plugins": [
				"@typescript-eslint",
				"react-refresh",
				"react"
			],
			"root": true,
			"rules": {},
			"settings": {
				"react": {
					"version": "detect"
				}
			}
}